#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int num;
    cout<<"Task number: ";
    cin>> num;
    switch(num){
case 1: 
{
    int a, b;
    cout<<"A: ";
    cin>>a;
    cout<<"B: ";
    cin>>b;
    if (a<b)
    for(int i=a; i<=b ;i++)
        for (int j=0; j<i; j++)
            cout << i;
    else cout<<"no";
    break;
}    
case 2: 
{
    int a, b, s;
    cout<<"A: ";
    cin>>a;
    cout<<"B: ";
    cin>>b;
    if (a>b){
    for(s=a; s>b ;)
        s=s-b;
    cout<< s;}
    else cout<<"no";
    break;
}    
case 3: 
{
    int n, s=1, k=1;
    cout<<"N: ";
    cin>>n;
    if (n>1)
    {
    while (k*(k+1)/2 <n)
    {
        ++k;
    }
    s=k*(k+1)/2;
    cout<< "K= "<<k<<"\nSumm= "<<s;
    }
    else cout <<"no";
    break;
}
case 4:
{
    int k=0;
    float p, s=1000;
    cout<<"P(0<p<25): ";
    cin>>p;
    if (p>0&p<25){
    while (s <= 1100) 
    {
        ++k;
        s+=s*p/100;
    }
    cout<< "K= "<<k<<"\nSumm= "<<s;}
    else cout <<"wrong";
    break;
}
case 5:
{
    int a, b;
    cout<<"Ð: ";
    cin>>a;
    cout<<"B: ";
    cin>>b;
    
    while (a!=0 & b!=0) 
    {
        if (a>b) a=a%b;
        else b=b%a;
    }
    cout<< "GCD= "<<a+b;
    break;
}
case 6:
{
    int n;
    cout<<"N: ";
    cin>>n;
    if (n>1)
    {int  f = 0,f1 = 1, f2 = 1, k = 2;
    while (f<n)
    {
        ++k;
        f = f2+f1;
        f2 = f1;
        f1 = f;
    }
    cout<< "K= "<<k;}
    else cout <<"no";
    break;
}
return 0;
}
}

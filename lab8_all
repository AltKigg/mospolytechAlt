//task1
#include <iostream>

using namespace std;

int main ()
{
    int b, k;
    cout<<"insert size in bytes";
    cin>>b;
    if (k%b==0)
    k=b/1024;
    else k=b/1024+1;
    cout<<"\nin kilobytes: "<< k;
    return 0;
}
//task2
#include <iostream>

using namespace std;

int main ()
{
    unsigned int a, b, k;
    cout<<"A=";
    cin>>a;
    cout<<"B=";
    cin>>b;
    if (a>b){
    k=a/b;
    cout<<"\nanswer is "<< k;}
    else cout<<"no";
    return 0;
}
//task3
#include <iostream>

using namespace std;

int main ()
{
    unsigned int a, b, k;
    cout<<"A=";
    cin>>a;
    cout<<"B=";
    cin>>b;
    if (a>b){
    k=a%b;
    cout<<"\nanswer is "<< k;}
    else cout<<"no";
    return 0;
}
//task4
#include <iostream>

using namespace std;

int main ()
{
    int a, b, k;
    cout<<"Number is ";
    cin>>a;
    if (a>9&a<100)
    {b=a/10;
    k=a%10;
    cout<<"\nanswer is "<< 10*k+b;}
    else cout<<"no";
    return 0;
}
//task5
#include <iostream>

using namespace std;

int main ()
{
    int a, b, k;
    cout<<"Number is ";
    cin>>a;
    if (a>99&a<1000)
    {b=a/100;
    a=(a-b*100)*10+b;
    cout<<"\nanswer is "<< a;}
    else cout<<"no";
    return 0;
}
